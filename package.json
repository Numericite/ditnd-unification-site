{
	"name": "ditnd-unification-site",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"prebuild": "react-dsfr update-icons",
		"build": "next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"predev": "react-dsfr update-icons",
		"dev": "next dev --turbo",
		"preview": "next build && next start",
		"start": "next start",
		"typecheck": "tsc --noEmit",
		"payload": "PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload",
		"seed:prod": "PAYLOAD_CONFIG_PATH=./src/payload/payload.config.ts payload run ./src/payload/seed/index.ts",
		"seed:dev": "NODE_ENV=development PAYLOAD_CONFIG_PATH=./src/payload/payload.config.ts PAYLOAD_DROP_DATABASE=true payload run ./src/payload/seed/index.ts",
		"test-chatbot:ab": "payload run tests/chabot/eval.ts -- --ab",
		"test-chatbot:prompt": "payload run tests/chabot/eval.ts -- --prompt="
	},
	"dependencies": {
		"@codegouvfr/react-dsfr": "1.29.0",
		"@emotion/react": "11.14.0",
		"@emotion/server": "11.11.0",
		"@emotion/styled": "11.14.1",
		"@huggingface/transformers": "^3.8.1",
		"@legendapp/state": "^2.1.15",
		"@mui/material": "7.3.6",
		"@payloadcms/db-postgres": "3.69.0",
		"@payloadcms/next": "3.69.0",
		"@payloadcms/richtext-lexical": "3.69.0",
		"@payloadcms/storage-s3": "3.69.0",
		"@payloadcms/translations": "^3.76.0",
		"@t3-oss/env-nextjs": "0.12.0",
		"@tanstack/react-query": "5.69.0",
		"@trpc/client": "11.0.0",
		"@trpc/next": "11.0.0",
		"@trpc/react-query": "11.0.0",
		"@trpc/server": "11.0.0",
		"better-auth": "1.3",
		"graphql": "16.12.0",
		"next": "16.1.0",
		"node-addon-api": "^8.5.0",
		"node-gyp": "^12.2.0",
		"payload": "3.69.0",
		"react": "19.0.0",
		"react-colorful": "^5.6.1",
		"react-dom": "19.0.0",
		"sharp": "^0.34.5",
		"superjson": "2.2.1",
		"tss-react": "4.9.19",
		"zod": "3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@types/node": "20.14.10",
		"@types/react": "19.0.0",
		"@types/react-dom": "19.0.0",
		"@types/sanitize-html": "^2.16.0",
		"typescript": "5.8.2"
	},
	"ct3aMetadata": {
		"initVersion": "7.40.0"
	},
	"packageManager": "yarn@1.22.22"
}
